{
  "name": "menu-search-modal",
  "private": false,
  "version": "1.0.0-alpha.4",
  "description": "A menu search modal component base on Ant Design Vue",
  "keywords": [
    "vue3",
    "ant-design-vue",
    "antdv"
  ],
  "author": {
    "name": "Max-Loo"
  },
  "main": "lib/MenuSearchModal/index.js",
  "module": "lib/MenuSearchModal/index.js",
  "typings": "lib/MenuSearchModal/index.d.ts",
  "files": [
    "lib/MenuSearchModal"
  ],
  "packageManager": "pnpm@7.3.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:packages": "rm -rf lib && vue-tsc -p tsconfig.packages.json && vite build --config vite.packages.config.ts",
    "preview": "vite preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "eslint": "eslint {src,packages}/**/*.{js,vue,ts,tsx} --fix",
    "stylelint": "stylelint src/**/*.{css,vue} --fix",
    "lint:init": "eslint --init",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.7",
    "lodash-es": "^4.17.21",
    "vue-types": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.7",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-import": "^14.1.0",
    "postcss-lit": "^0.4.3",
    "rollup": "^2.75.7",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "stylelint": "^14.8.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vitest": "^0.17.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.29.8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
